"settings" : {
    "columns" : 2
},
"metadata" : {
    "keys" : [ "title", "subtitle", "artist", "album"]
},
"pdf" : {
    "formats" : {
        "title" : {
            "title"    : [ "", "%{title}%{artist| (%{})}", "" ],
            "subtitle" : [ "", "%{subtitle}", "" ],
            "footer"   : [ "", "%{title}", "%{page}" ]
        }
    },
    "chorus" : {
        "indent" : 0,
        "bar" : {
            "offset" : 8,
            "width"  : 1,
            "color"  : "foreground"
        },
        "recall" : {
            "tag"   : "Chorus",
            "type"  : "comment",
            "quote" : false
        }
    },
    "fonts" : {
        "text" : {
            "size" : 12
        },
        "chord" : {
            "size" : 10
        },
        "comment" : {
            "size" : 8
        },
        "comment_italic" : {
            "size" : 8
        },
        "comment_box" : {
            "size" : 8
        }
    },
    "columnspace": 10
},
"notes" : {
    "system" : "german",
    "sharp" : [ "C", "Cis",
                "D", "Dis",
                "E",
                "F", "Fis",
                "G", "Gis",
                "A", "Ais",
                "H"
              ],
    "flat" :  [ "C",
                "Des", "D",
                "Es",  "E",
                "F",
                "Ges", "G",
                "As",  "A",
                "B",   "H"
              ]
},
"chords" : [{
    "name"    : "Ami",
    "display" : "Ami",
    "base"    : 1,
    "frets"   : [ x, 0, 2, 2, 1, 0 ]
}],
"contents" : [{
    "fields"   : [ "sorttitle", "artist" ],
    "label"    : "Contents by Title",
    "line"     : "%{title}%{artist| (%{})}",
    "pageno"   : "%{page}",
    "omit"     : false,
    "template" : "stdtoc"
}]

