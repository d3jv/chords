"settings" : {
    "columns" : 2
},
"metadata" : {
    "keys" : [ "title", "subtitle", "artist", "album"]
},
"pdf" : {
    "pagealign-songs" : 0,
    "formats" : {
        "title" : {
            "title"    : [ "", "%{title}%{artist| (%{})}", "" ],
            "subtitle" : [ "", "%{subtitle}", "" ],
            "footer"   : [ "", "%{title}", "%{page}" ]
        }
    },
    "chorus" : {
        "indent" : 0,
        "bar" : {
            "offset" : 8,
            "width"  : 1,
            "color"  : "foreground"
        },
        "recall" : {
            "tag"   : "Chorus",
            "type"  : "comment",
            "quote" : true
        }
    },
    "fonts" : {
        "text" : {
            "size" : 12
        },
        "chord" : {
            "size" : 10
        },
        "comment" : {
            "size" : 8
        },
        "comment_italic" : {
            "size" : 8
        },
        "comment_box" : {
            "size" : 8
        }
    },
    "columnspace": 10
},
"notes" : {
  "system" : "european",
  "sharp" : [
      "C", [ "C#", "Cis", "C♯" ],
      "D", [ "D#", "Dis", "D♯" ],
      "E",
      "F", [ "F#", "Fis", "F♯" ],
      "G", [ "G#", "Gis", "G♯" ],
      "A", [ "A#", "Ais", "A♯" ],
      "H"
  ],
  "flat" : [
                                    "C",
      [ "Db", "Des",        "D♭" ], "D",
      [ "Eb", "Es",  "Ees", "E♭" ], "E",
                                    "F",
      [ "Gb", "Ges",        "G♭" ], "G",
      [ "Ab", "As",  "Aes", "A♭" ], "A",
      [ "B",  "B",          "B" ],  "H"
    ]
},
"chords" : [
    { "name" : "E7", "base" : 1, "frets" : [ 0, 2, 0, 1, 0, 0 ] }
],
"contents" : [{
    "fields"   : [ "sorttitle", "artist" ],
    "label"    : "Contents by Title",
    "line"     : "%{title}%{artist| (%{})}",
    "pageno"   : "%{page}",
    "omit"     : false,
    "template" : "stdtoc"
}]

